:javascript
  window.authenticity_token = "#{form_authenticity_token}";
  window.userRouter = new Agreatfirstdate.Routers.UserRouter({user: #{@profile.to_json scope: :profile}, me: #{current_user.profile.to_json scope: :self}, owner: false});
  window.userRouter.show();
  window.router = new Agreatfirstdate.Routers.PillarsRouter({pillars: #{@pillars.to_json scope: :profile}, owner: false});
  Backbone.history.start();