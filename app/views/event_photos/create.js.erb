<%= remotipart_response do %>
  var collection = window.router.pillars.get(<%= params[:pillar_id] %>).eventItems.currentModel.eventPhotos, eventPhoto;

  <% @event_photos.each do |event_photo| %>
    <% if event_photo.persisted? %>
      eventPhoto = <%= event_photo.to_json(scope: :profile).html_safe %>;
      collection.add(eventPhoto);
    <% else %>
      console.log('fail');
      $('.upload-status_').html("<%= escape_javascript(event_photo.errors.messages.values.join('<br/>')) %>")
    <% end %>
  <% end %>
<% end %>
