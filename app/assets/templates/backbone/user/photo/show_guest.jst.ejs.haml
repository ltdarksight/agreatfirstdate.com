<% if (!avatar) { %>
%span No Photo
<% }else { %>
<% if(avatars.length>0){ %>
.carousel#avatar_carousel
  .carousel-inner
    <% _.each(avatars, function(avatar){ %>
    .item
      %img{src: '<%= avatar.image.thumb.url %>?<%= (new Date()).getTime() %>'}
    <% }); %>
<% }else{ %>
%img{src: '<%= avatar.image.thumb.url %>?<%= (new Date()).getTime() %>'}
<% } %>
<% } %>
